
[measureSamplingRate]
measure=plugin
plugin=audioLevel
type=format
regExpSubstitute=1
substitute="(Hz.*)$":""
updateDivider=-1

[mAudio]
measure=plugin
plugin=audiolevel
port=output
FFTSize=(floor([measureSamplingRate] * #accuracy# / #FPS#))
FFTOverlap=(floor([measureSamplingRate] * (#accuracy# - 1) / #FPS#))
FFTattack=0
FFTdecay=50
bands=14
freqmin=20
freqmax=16500
sensitivity=#sensitivity#
disabled=(1-#visDisp#)
dynamicvariables=1

[b1]
measure=plugin
plugin=audiolevel
parent=mAudio
type=band
averagesize=#avgSize#
disabled=(1-#visDisp#)
bandidx=2
